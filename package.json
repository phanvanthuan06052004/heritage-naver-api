{
  "name": "nodejs-expressjs-mongodb-base-project",
  "private": true,
  "version": "1.0.0",
  "author": "PromptEngineers",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && cross-env BUILD_MODE=production node -r dotenv/config ./build/src/server.js",
    "dev": "cross-env BUILD_MODE=dev nodemon --exec babel-node -r dotenv/config ./src/server.js",
    "scriptsImportData": "nodemon --exec babel-node -r dotenv/config ./src/scripts/import-heritage-data",
    "scriptsAddSlug": "nodemon --exec babel-node -r dotenv/config ./src/scripts/addSlug",
    "test-socket": "nodemon --exec babel-node -r dotenv/config ./src/test/test-socket-client.js",
    "create-chatrooms": "nodemon --exec babel-node -r dotenv/config ./src/scripts/create-chatrooms.js",
    "test-qdrant": "node test-qdrant.js",
    "test-rag-heritage": "node test-rag-with-heritage-id.js",
    "upload-heritage-to-qdrant": "babel-node -r dotenv/config ./src/scripts/upload-heritage-to-qdrant.js",
    "create-qdrant-index": "babel-node -r dotenv/config ./src/scripts/create-qdrant-index.js",
    "prepare-heritage": "babel-node -r dotenv/config ./src/scripts/prepare-heritage-text.js",
    "benchmark": "babel-node -r dotenv/config src/benchmark/runBenchmark.js",
    "benchmark:mock": "babel-node -r dotenv/config src/benchmark/runBenchmark.js --mock"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@qdrant/js-client-rest": "^1.15.1",
    "async-exit-hook": "^2.0.1",
    "bcryptjs": "^3.0.2",
    "chromadb": "^3.1.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dot": "^1.1.3",
    "dotenv": "^16.4.7",
    "env": "^0.0.2",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.15.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "path": "^0.12.7",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@types/node": "^22.14.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.47.0",
    "nodemon": "^3.0.1"
  }
}
